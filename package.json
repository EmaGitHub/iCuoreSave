{
  "name": "EUCouncil",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "serve:mocks": "ENV=mocks ionic-app-scripts serve -p 8080",
    "serve:dev": "ENV=dev ionic-app-scripts serve -p 8080",
    "serve:prod": "ENV=prod ionic-app-scripts serve -p 8080",
    "build:android:coll": "ENV=coll ionic cordova build android --prod --no-interactive --release",
    "build:android:prod": "ENV=prod ionic cordova build android --prod --no-interactive --release",
    "build:ios:coll": "ENV=coll ionic cordova build ios --prod --no-interactive",
    "build:ios:prod": "ENV=prod ionic cordova build ios --prod --no-interactive",
    "run:ios:coll": "ENV=coll ionic cordova run ios --no-interactive",
    "run:android:coll": "ENV=coll ionic cordova run android --no-interactive",
    "start-mocks": "./node_modules/.bin/distribute serve ./mocks"
  },
  "dependencies": {
